    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "closure-webpack-plugin": "^2.3.0",
    "create-esm": "^1.0.18",
    "eslint": "^7.4.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-jsdoc": "^29.2.0",
    "google-closure-compiler": "^20200719.0.0",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.3.0",
    "inspect-loader": "^1.0.0",
    "mocha": "^8.0.1",
    "rollup": "^2.21.0",
