{
  "name": "gdev",
  "version": "2.0.0",
  "description": "Javascript development workspaces built around Closure Compiler.",
  "main": "dist/node.cjs",
  "module": "dist/node.mjs",
  "type": "module",
  "bin": {
    "gdev": "dist/cli.min.cjs"
  },
  "scripts": {
    "run:rollup": "yarn rollup -c",
    "run:webpack": "yarn webpack --config webpack.config.cjs",
    "run:compile": "yarn gulp",
    "build": "yarn run:rollup && yarn run:webpack && yarn run:compile",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TeleworkInc/gdev.git"
  },
  "keywords": [
    "closure",
    "compiler",
    "google",
    "javascript"
  ],
  "author": "Christian Lewis <hello@trytelework.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TeleworkInc/gdev/issues"
  },
  "homepage": "https://github.com/TeleworkInc/gdev#readme",
  "dependencies": {
    "ascii-file-tree": "^1.2.2",
    "call-bash": "^1.4.0",
    "chalk": "^4.1.0",
    "chokidar": "^3.4.0",
    "commander": "^5.1.0",
    "figlet": "^1.4.0",
    "filetouch": "^1.4.0",
    "ora": "^4.0.4",
    "touch": "^3.1.0"
  },
  "devDependencies": {
    "@ampproject/rollup-plugin-closure-compiler": "^0.26.0",
